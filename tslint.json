{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "quotemark": false,
    "no-console": {
      "options": ["warn", "info"]
    },
    "trailing-comma": true,
    "arrow-parens": false,
    "object-literal-key-quotes": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
